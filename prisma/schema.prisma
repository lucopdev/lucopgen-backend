datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         Int        @id     @default(autoincrement())
  name       String
  email      String     @unique
  phone      String
  password   String
  accounts   Account[]
}

model Account {
  id         Int        @id     @default(autoincrement())
  name       String
  login      String
  password   String
  ownerId    Int
  owner      User       @relation(fields: [ownerId], references: [id])
}